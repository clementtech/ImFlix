{
    "manifest_version": 3,
    "name": "ImFlix",
    "version": "0.1",
    "description": "Script that uses vidsrc to get the best source for movies.",

    "content_scripts": [
        {
            "matches": ["*://*.imdb.com/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
    

}